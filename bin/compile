#!/bin/bash

if [ "$#" -lt 1 ]; then
    echo "Specify a source to compile."
    exit -1
fi

SOURCE=$1

mkdir -p exe
gcc src/$SOURCE.c                 \
  -I ./src                        \
  -Wall                           \
  -Wno-override-init-side-effects \
  -Wno-unused-label               \
  -Werror                         \
  ${@:2}
